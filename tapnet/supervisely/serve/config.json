{
    "name": "TAP-Net object tracking",
    "type": "app",
    "version": "2.0.0",
    "icon_cover": true,
    "categories": [
        "neural network",
        "videos",
        "detection & tracking",
        "serve"
    ],
    "description": "serve and use in videos annotator",
    "docker_image": "supervisely/tapnet:1.0.0",
    "main_script": "src/main.py",
    "task_location": "application_sessions",
    "isolate": true,
    "headless": true,
    "need_gpu": true,
    "instance_version": "6.7.16",
    "restart_policy": "on_error",
    "session_tags": [
        "sly_video_tracking"
    ],
    "community_agent": false,
    "allowed_shapes": [
        "polygon",
        "point",
        "rectangle",
        "graph"
    ]
}